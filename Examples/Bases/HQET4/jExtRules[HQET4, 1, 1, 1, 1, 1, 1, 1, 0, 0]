j[HQET4, (n1_)?Positive, (n2_)?Positive, (n3_)?Positive, (n4_)?Positive, 
  (n5_)?Positive, (n6_)?Positive, (n7_)?Positive, (n8_)?NonPositive, 
  (n9_)?NonPositive] :> Expand[j[HQET3, n1, 0, n2, n3, n5, n4, n6, n7, 0]/
   ((j[HQET3, -1, 0, 0, 0, 0, 0, 0, 0, 0]/2 - 
      j[HQET3, 0, -1, 0, 0, 0, 0, 0, 0, 0]/2)^n9*
    (-j[HQET3, 0, 0, 0, -1, 0, 0, 0, 0, 0] + j[HQET3, 0, 0, 0, 0, 0, 0, -1, 
       0, 0] - 2*j[HQET3, 0, 0, 0, 0, 0, 0, 0, 0, -1])^n8)]
